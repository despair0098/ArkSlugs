    digitalWrite(RST, HIGH);
    delay(200);
}

void initialize(){
    hardwareReset();
    digitalWrite(CS, LOW); // SLPOUT
    digitalWrite(DC, LOW);
    SPI.transfer(0x11);  
    digitalWrite(CS, HIGH);
    
    delay(150);
    
    digitalWrite(CS, LOW); // color mode
    digitalWrite(DC, LOW);
    SPI.transfer(0x3A);
    
    
    // mode 5
    digitalWrite(DC, HIGH);
    SPI.transfer(0x05);
    digitalWrite(CS, HIGH);

    digitalWrite(CS, LOW);
    digitalWrite(DC, LOW); // DISPON
    SPI.transfer(0x29);
    digitalWrite(CS, HIGH);
}
