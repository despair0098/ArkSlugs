void printSprite(){
    
    digitalWrite(CS, LOW);
    digitalWrite(DC, LOW);
    SPI.transfer(0x2A); // setting column address
    digitalWrite(CS, HIGH);

    digitalWrite(CS, LOW);
    digitalWrite(DC, HIGH);
    SPI.transfer(0x00);
    SPI.transfer(2);
    SPI.transfer(0x00);
    SPI.transfer(129);
    digitalWrite(CS, HIGH);

    

    digitalWrite(CS, LOW);
    digitalWrite(DC, LOW);
    SPI.transfer(0x2B); // setting row address
    digitalWrite(CS, HIGH);

    digitalWrite(CS, LOW);
    digitalWrite(DC, HIGH);
    SPI.transfer(0x00);
    SPI.transfer(1);
    SPI.transfer(0x00);
    SPI.transfer(128);
    digitalWrite(CS, HIGH);

    digitalWrite(CS, LOW);
    digitalWrite(DC, LOW);
    SPI.transfer(0x2C);

    

    digitalWrite(DC, HIGH);
    int len = sizeof(epd_bitmap_Safeimagekit_resized_imgpng) / sizeof(char);
    for(int i = 0; i < (128 * 132); i++){
      SPI.transfer(0b11111000);
      SPI.transfer(0b00011111);
    }

    digitalWrite(CS, HIGH);

    digitalWrite(CS, LOW);
    digitalWrite(DC, LOW);
    SPI.transfer(0x2C);
    
    digitalWrite(DC, HIGH);
    char mask = 0x80;
    for(int i = 0; i < len; i++){
      for(int j = 0; j < 8; j++){
        if(mask & pgm_read_byte_near(epd_bitmap_Safeimagekit_resized_imgpng + i)){
            SPI.transfer16(0xFFFF);
        } else {
            SPI.transfer16(0x0000);
        }
        mask = mask >> 1;   
      }
      mask = 0x80; 
    }
    
    
    digitalWrite(CS, HIGH);
    
    
}

